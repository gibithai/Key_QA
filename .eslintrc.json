{
  "env": {
    "browser": true,
    "es2021": true,
    "cypress/globals": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "cypress", "prettier"
  ],
  "extends": [
    "prettier",
    "plugin:prettier/recommended",
    "eslint:recommended",
    "plugin:cypress/recommended",
    "plugin:prettier/recommended",
    "standard-with-typescript"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": false,
        "singleQuote": true,
        "trailingComma": "off",
        "printWidth": 90
      }
    ]
  }
}